{
  "name": "chao-ui",
  "version": "0.1.1",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name chao-ui --dest lib packages/index.js"
  },
  "description": "A Web UI component library similar to desktop experience",
  "keywords": [
    "component",
    "components",
    "design",
    "window",
    "desktop",
    "vue",
    "vue-component",
    "ui",
    "黑超特攻"
  ],
  "homepage": "https://github.com/diablozzc/chao-ui",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/diablozzc/chao-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:diablozzc/chao-ui.git"
  },
  "contributors": [
    "diablozzc"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "velocity-animate": "^1.5.2",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.0",
    "@vue/cli-service": "^4.5.0",
    "babel-eslint": "^10.1.0",
    "element-ui": "^2.15.2",
    "eslint": ">=7.11.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.23.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "vue-router": "^3.5.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "quote-props": [
        "error",
        "consistent"
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "main": "lib/chao-ui.umd.min.js"
}
